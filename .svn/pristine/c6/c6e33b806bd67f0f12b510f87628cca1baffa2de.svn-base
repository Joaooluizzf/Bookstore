package br.com.app.authentication;

public class AccountTokenAuth {

	private String user;
	private String token;

	public AccountTokenAuth() {
	}

	public AccountTokenAuth(String user, String token) {
		this.user = user;
		this.token = token;
	}

	public String getUser() {
		return user;
	}

	public void setUser(String user) {
		this.user = user;
	}

	public String getToken() {
		return token;
	}

	public void setToken(String token) {
		this.token = token;
	}

}
